{
  "rules": {
    "users": {
      "$uid": {
        "gotout": {
          ".write": "$uid === auth.uid"
        }
      }
    },
    "targets": {
      "$email": {
        ".read": "$email === auth.email.replace('.','')"
      }
    },
    "out": {
      "$email": {
        ".read": "$email === auth.email.replace('.','')"
      }
    },
    "names": {
      ".read": "auth.email.matches(/.*@milton\\.edu$/)"
    },
    "numTags": {
      ".read": "auth.email.matches(/.*@milton\\.edu$/)"
    },
    "tags": {
      ".read": "auth.email.matches(/.*@milton\\.edu$/)"
    },
    ".read": "false",
    ".write": "false"
  }
}
